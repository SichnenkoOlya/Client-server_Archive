package by.bsuir.archive.dao;

import by.bsuir.archive.dao.exception.DAOException;
import by.bsuir.server.domain.Matter;

public interface DAOMatter {
	
	void createMatter(String surname, String name, int age, String univer, int numberOfGroup) 
			throws DAOException;	
	Matter findMatter(int idMatter) throws DAOException;	
	void changeMatter(int idMatter,String newSurname, String newName, int newAge, String newUniver, 
			int newNumberOfGroup) throws DAOException;

}
